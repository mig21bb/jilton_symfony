<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\JiltonOcupaciones" table="jilton_ocupaciones">
    <indexes>
      <index name="fk_JILTON_OCUPACIONES_JILTON_ROOMS1_idx" columns="numeroRoom,idHotel,numeroPlanta"/>
      <index name="fk_JILTON_OCUPACIONES_JILTON_CLIENTES1_idx" columns="idCliente"/>
    </indexes>
    <id name="idocupaciones" type="integer" column="idOcupaciones">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fentrada" type="datetime" column="fEntrada" nullable="false"/>
    <field name="fsalida" type="datetime" column="fSalida" nullable="true"/>
    <field name="fcreacion" type="datetime" column="fCreacion" nullable="true"/>
    <field name="activo" type="integer" column="activo" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="numeroroom" target-entity="JiltonRooms" fetch="LAZY">
      <join-columns>
        <join-column name="numeroRoom" referenced-column-name="numeroRoom"/>
        <join-column name="idHotel" referenced-column-name="idHotel"/>
        <join-column name="numeroPlanta" referenced-column-name="numeroPlanta"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idcliente" target-entity="JiltonClientes" fetch="LAZY">
      <join-columns>
        <join-column name="idCliente" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\JiltonRooms" table="jilton_rooms">
    <indexes>
      <index name="fk_JILTON_ROOMS_JILTON_PLANTAS1_idx" columns="idHotel"/>
      <index name="fk_JILTON_ROOMS_JILTON_PLANTAS2_idx" columns="numeroPlanta"/>
    </indexes>
    <id name="numeroroom" type="integer" column="numeroRoom"/>
    <id name="idHotel" type="integer" column="idHotel"/>
    <id name="numeroplanta" type="integer" column="numeroPlanta"/>
    <field name="activa" type="integer" column="activa" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="fcreacion" type="datetime" column="fCreacion" nullable="true"/>
    <field name="fmodificacion" type="datetime" column="fModificacion" nullable="true"/>
    <field name="fborrado" type="datetime" column="fBorrado" nullable="true"/>
    <many-to-one field="idHotel" target-entity="JiltonHotel" fetch="LAZY">
        <join-column  referenced-column-name="idHotel" />
    </many-to-one>
  </entity>
</doctrine-mapping>
